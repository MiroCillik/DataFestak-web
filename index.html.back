			$teardrop = $('<div id="drop">');
//			$heart = $('<div class="heart">');
//			$teardrop.append($heart);
			$teardrop.css("display", "none");

			$('#dots').append($teardrop);

			var coors = [
				{
					top: 81,
					left: 134
				},
