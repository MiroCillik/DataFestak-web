			$.each(coors, function(i, v) {
				var $dot = $('<div class="dot">');

				$dot.css('top', v.top);
				$dot.css('left', v.left);

				$('#dots').append($dot);
			});

			$('.dot').mouseover(function() {
				var position = $(this).position();

				$('#drop').css({
				});

				$('#drop').show().animate({
					top: position.top - 58,
					left: position.left -14
				}, 'toggle');
			}).mouseout(function() {
//				$('#drop').hide();
			});

			$('.dot').first().mouseover();


			$('#carousel').carousel('pause');

